<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.myclothing.app"
        versionCode="1" 
        version   = "1.0.0">

    <!-- versionCode is optional and Android only -->

    <icon src="images/logo1.fw.png" />
    
    <name>What Should I Wear</name>

    <description>
        An app that will allow users to take pictures of their clothing and accessories and that will allow them to mix and match so they can create and see the outfit before they even try it on.
    </description>

    <author href="" email="jrasque@hotmail.com">
        Janet Rasque
    </author>
    
    <!--android -->
    <plugin name="Capture" value="org.apache.cordova.Capture"/>
    <plugin name="Camera" value="org.apache.cordova.CameraLauncher" />
    
    <!--ios -->
    <plugin name="Capture" value="CDVCapture" />
    <plugin name="Camera" value="CDVCamera" />
    

</widget>
